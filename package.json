    "stylelint-config-standard-scss": "13.x",
    "stylelint-no-unsupported-browser-features": "8.x"
  },
  "browserslist": [
    "last 2 versions",
    "Explorer >= 10",
    "iOS >= 9",
    "Android >= 5"
  ],
  "devDependencies": {
    "stylelint": "^16.6.1"
  },
  "keywords": [
    "stylelint",
    "stylelint-config",
    "netcentric"
