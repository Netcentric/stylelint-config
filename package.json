{
  "name": "@netcentric/stylelint-config",
  "description": "Netcentric's coding and style rules for Stylelint",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Netcentric/stylelint-config"
  },
  "main": "index.js",
  "scripts": {
    "lint": "stylelint test/*.scss"
  },
  "peerDependencies": {
    "stylelint": "16.x"
  },
  "stylelint": {
    "extends": "./index.js"
  },
  "dependencies": {
